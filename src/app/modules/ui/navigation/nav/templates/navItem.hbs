<a href="#{{ route }}">
  <i class="icon icon-{{ icon }}"></i>
  <span>{{ label }}</span>
</a>

{{#if sections}}
<ul class="modulesNav">
  {{#each sections}}
    <li>
      <a href="#{{ route }}">
        <i class="icon icon-{{ icon }}"></i>
        <span>{{ label }}</span>
      </a>
    </li>
  {{/each}}
</ul>
{{/if}}
